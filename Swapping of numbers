#include<stdio.h>
int main()
{
    int num1,num2,temp;
    printf("Enter the 1st no : ");
    scanf("%d",&num1);
    printf("Enter the 2nd no : ");
    scanf("%d",&num2);
    printf("Original numbers :\n");
    printf("Num1 = %d\n", num1);
    printf("Num2 = %d\n", num2);
    temp = num1;
    num1 = num2;
    num2 = temp;
    printf("After swapping :\n");
    printf("Num1 = %d\n", num1);
    printf("Num2 = %d\n", num2);
    return 0;
}
